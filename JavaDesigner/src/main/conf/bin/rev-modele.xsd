          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="Public" />
            <xs:enumeration value="Protected" />
            <xs:enumeration value="Private" />
            <xs:enumeration value="Visibility_Undefined" />
            <xs:enumeration value="Package_Visibility" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
