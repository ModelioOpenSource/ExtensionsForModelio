      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="enumeration" />
        <xs:element ref="template-parameter" />
        <xs:element ref="use" />
        <xs:element ref="interface" />
        <xs:element ref="data-type" />
        <xs:element ref="class" />
        <xs:element ref="stereotype" />
